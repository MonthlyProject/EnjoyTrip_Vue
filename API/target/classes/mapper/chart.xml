<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
	
<mapper namespace="com.ssafy.trip.chart.model.mapper.ChartMapper">
	
	<select id="getSidoName" resultType="string">
		select sido_name
		from sido
	</select>
	
	<select id="getSidoCnt" resultType="int">
		select cnt
		from sido
	</select>
	
	<select id="getGugunName" resultType="string">
		select gugun_name
		from gugun
	</select>
	
	<select id="getGugunCnt" resultType="int">
		select cnt
		from gugun
	</select>
	
	<select id="getContentName" resultType="string">
		select content_type_id
		from content_type
	</select>
	
	<select id="getContentCnt" resultType="int">
		select cnt
		from content_type
	</select>
	
</mapper>